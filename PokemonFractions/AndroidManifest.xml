<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pokemonfractions"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="12"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/poke_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" > 
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>
        </activity>
        <activity
            android:name=".PokeFling"
            android:label="@string/title_activity_poke_fling" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

            </intent-filter>
        </activity>
        <activity
            android:name=".Tutorial"
            android:label="@string/title_activity_tutorial" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

            </intent-filter>
        </activity>
        <activity
            android:name=".MasterAddition"
            android:label="@string/title_activity_master_addition" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FlingScore"
            android:label="@string/title_activity_fling_score" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                
            </intent-filter>
        </activity>
        <activity
            android:name=".MasterScore"
            android:label="@string/title_activity_master_score" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

            </intent-filter>
        </activity>
    </application>

</manifest>